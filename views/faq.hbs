<style>
    p {
	    font-family: 'Monaco', sans-serif;
	    text-align:  center;
	    font-size:   1.5em;
    }

    p a {
	    color:  #C0392B;
	    cursor: pointer;
    }

    p span {
	    color: #D1D8E0;
    }
  </style>
<div id="preloader" class="visible"></div>
<header>
    <div class="navbar-fixed">
      <nav>
        <div class="nav-wrapper row">
          <a class="brand-logo center" href="/"><4<span>P</span>></a>
        </div>
      </nav>
    </div>
  </header>
  <div class="container">
    <main>
      <h1 class="center-align" style="margin-bottom: 2rem;">FAQ</h1>
      <div class="row">
        <div class="col s12">
	        {{#if exists}}
		        <ul class="collapsible expandable">
			        {{#each faqs}}
				        <li>
					        <div class="collapsible-header">{{this.title}}</div>
					        <div class="collapsible-body"><span>{{this.description}}</span></div>
				        </li>
			        {{/each}}
		        </ul>
	        {{else}}
		        <h4 class="center-align">No FAQs yet...</h4>
	        {{/if}}
        </div>
      </div>
      <p><a onclick="history.go(-1)">go back</a> <span>|</span> <a href="/">index</a></p>
    </main>
  </div>